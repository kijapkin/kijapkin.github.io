$(function(){
	$("a[href^='#']").click(function(){
	        var _href = $(this).attr("href");
	        $("html, body").animate({scrollTop: $(_href).offset().top - 80});
	        return false;
	});
});




function displaylightbox(url, options)
{
   options.padding = 0;
   options.autoScale = true;
   options.href = url;
   options.type = 'iframe';
   $.fancybox(options);
}
$(document).ready(function()
{
   var Carousel1Opts =
   {
      delay: 10000,
      duration: 700,
      easing: 'linear',
      mode: 'fade',
      direction: '',
      scalemode: 1,
      pagination: true,
      pagination_img_default: '//best-gooods.ru/easybreath/files/img/page_default.png',
      pagination_img_active: '//best-gooods.ru/easybreath/files/img/page_active.png',
      start: 0
   };
   $("#Carousel1").carouseleffects(Carousel1Opts);
   $('a[href*=#Bookmark1]').click(function(event)
   {
      event.preventDefault();
      $('html, body').stop().animate({ scrollTop: $('#wb_Bookmark1').offset().top }, 600, 'linear');
   });
   $("a[data-rel='prettyPhoto_PhotoGallery1[PhotoGallery1]']").attr('rel', 'prettyPhoto_PhotoGallery1[PhotoGallery1]');
   $("a[rel^='prettyPhoto_PhotoGallery1']").prettyPhoto({theme:'facebook',social_tools:false});
   $("a[data-rel='prettyPhoto_PhotoGallery2[PhotoGallery2]']").attr('rel', 'prettyPhoto_PhotoGallery2[PhotoGallery2]');
   $("a[rel^='prettyPhoto_PhotoGallery2']").prettyPhoto({theme:'facebook',social_tools:false});
   $("a[data-rel='prettyPhoto_PhotoGallery3[PhotoGallery3]']").attr('rel', 'prettyPhoto_PhotoGallery3[PhotoGallery3]');
   $("a[rel^='prettyPhoto_PhotoGallery3']").prettyPhoto({theme:'facebook',social_tools:false});
   $("a[data-rel='prettyPhoto_PhotoGallery4[PhotoGallery4]']").attr('rel', 'prettyPhoto_PhotoGallery4[PhotoGallery4]');
   $("a[rel^='prettyPhoto_PhotoGallery4']").prettyPhoto({theme:'facebook',social_tools:false});
});
